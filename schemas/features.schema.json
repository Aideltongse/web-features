{
  "$schema": "http://json-schema.org/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-z0-9-]*$": {
      "type": "object",
      "properties": {
        "spec": {
          "type": "string",
          "description": "Specification URL",
          "format": "uri"
        },
        "caniuse": {
          "type": "string",
          "description": "caniuse.com identifier"
        },
        "usage": {
          "type": "string",
          "description": "Feature usage metric URL",
          "format": "uri"
        }
      },
      "required": [
        "spec"
      ],
      "additionalProperties": false
    }
  },
  "additionalProperties": false
}
